{
    "env": {
        "test": {
            "presets": [
                ["env"]
            ],
            "ignore": [
                "**/*.test.js"
            ]
        },
        "commonjs": {
            "presets": [
                ["env", {
                    "useBuiltIns": false
                }]
            ],
            "ignore": [
                "**/*.test.js"
            ]
        },
        "es": {
            "presets": [
                ["env", {
                    "useBuiltIns": false,
                    "modules": false
                }]
            ],
            "plugins": [
                "external-helpers"
            ]
        }
    }
}