{
  "name": "resclient",
  "version": "1.1.0",
  "description": "Client implementation of the RES-Client Protocol.",
  "main": "./index.js",
  "scripts": {
    "build": "npm run clean && npm run eslint && npm run test && npm run docs && npm run babel",
    "babel": "babel src --out-dir ./",
    "test": "jest src --coverage",
    "clean": "rimraf class && rimraf def && rimraf coverage && rimraf docs/* && rimraf *.js",
    "docs": "jsdoc2md -f ./src/class/ResClient.js -f ./src/class/ResCollection.js -f ./src/class/ResModel.js -f ./src/def/eventCallback.js > ./docs/docs.md",
    "eslint": "eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jirenius/resclient"
  },
  "author": "Samuel Jir√©nius <samuel@jirenius.com>",
  "license": "MIT",
  "homepage": "https://github.com/jirenius/resclient",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "jsdoc-to-markdown": "^4.0.1",
    "mock-socket": "^7.1.0"
  },
  "dependencies": {
    "modapp": "^2.2.0",
    "modapp-utils": "^1.1.0"
  }
}
